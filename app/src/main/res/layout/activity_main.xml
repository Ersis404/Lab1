<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editString"
            app:layout_constraintTop_toBottomOf="@+id/textView2" android:layout_marginBottom="21dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/buttonNext"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Lab1 get Text "
            android:id="@+id/textView2"
            android:layout_marginTop="102dp"
            android:layout_marginBottom="20dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/editString" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Lab2"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button5"
            android:onClick="onClickLab2"
            app:layout_constraintTop_toBottomOf="@+id/buttonNext" app:layout_constraintEnd_toEndOf="@+id/button12"
            app:layout_constraintBottom_toTopOf="@+id/button12" app:layout_constraintStart_toStartOf="@+id/button12"/>
    <Button
            android:text="Lab4"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button2"
            android:onClick="onClickLab4"
            app:layout_constraintTop_toBottomOf="@+id/button" android:layout_marginBottom="274dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:id="@+id/button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Lab3.2"
            android:onClick="onClickNewVersionDB"
            app:layout_constraintTop_toBottomOf="@+id/button12" app:layout_constraintEnd_toEndOf="@+id/button2"
            app:layout_constraintBottom_toTopOf="@+id/button2" app:layout_constraintStart_toStartOf="@+id/button2"/>
    <Button
            android:text="Get Text"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/buttonNext"
            app:layout_constraintEnd_toEndOf="@+id/button5"
            app:layout_constraintBottom_toTopOf="@+id/button4" app:layout_constraintStart_toStartOf="@+id/button5"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="207dp"
            android:layout_marginBottom="201dp"/>
    <Button
            android:text="Lab3.1"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/button12"
            android:onClick="onClickLab3"
            app:layout_constraintTop_toBottomOf="@+id/button5" app:layout_constraintEnd_toEndOf="@+id/button"
            app:layout_constraintBottom_toTopOf="@+id/button" app:layout_constraintStart_toStartOf="@+id/button"/>
    <Button
            android:text="Lab5"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:id="@+id/button4"
            android:onClick="onClickLab5"
            app:layout_constraintTop_toBottomOf="@+id/buttonNext" android:layout_marginBottom="225dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="1dp" android:layout_marginEnd="1dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>